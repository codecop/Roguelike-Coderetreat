{
    "name": "codecop/rogue-template-lumen",
    "type": "project",
    "require": {
        "php": ">=7.3.0",
        "laravel/lumen-framework": "8.3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "9.6.*",
        "mockery/mockery": "1.6.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app\/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests\/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "tests": "phpunit"
    },
    "config": {
        "optimize-autoloader": true
    }
}